{
  "name": "@kingdarboja/serverless-plugin-typescript",
  "version": "1.3.0",
  "author": {
    "name": "Prisma Labs",
    "url": "https://www.prisma.io/"
  },
  "maintainers": [
    {
      "name": "KingDarBoja",
      "url": "https://github.com/KingDarBoja/"
    }
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "pretest": "npm run lint",
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "build:check": "tsc",
    "format": "prettier --loglevel error --write \"{src,tests}/**/*.ts\"",
    "test": "jest",
    "lint": "eslint --ext ts .",
    "prepublishOnly": "npm format && npm run lint && npm test",
    "preversion": "npm run format && npm run lint && npm test",
    "version": " git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KingDarBoja/serverless-plugin-typescript.git"
  },
  "keywords": [
    "serverless",
    "serverless plugin",
    "typescript",
    "aws lambda"
  ],
  "engines": {
    "node": ">=10.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "8.1.0",
    "@types/jest": "25.1.3",
    "@types/lodash": "4.14.149",
    "@types/node": "13.7.4",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsdoc": "^21.0.0",
    "eslint-plugin-prefer-arrow": "^1.1.7",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "25.1.0",
    "mock-fs": "4.10.4",
    "prettier": "^2.0.2",
    "ts-jest": "25.2.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "fs-extra": "^8.1.0",
    "globby": "^11.0.0",
    "lodash": "^4.17.15"
  }
}
